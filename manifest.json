{
    "name": "To the stars",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "permissions":[
      "http://*/",
      "https://*/",
      "storage"
    ],
    "browser_action": {
      "default_icon": "Icon-128.png",
      "default_popup":"popup.html"
    },
    "icons": {
      "128": "Icon-128.png"
    },
    "content_scripts":[{
      "matches":["<all_urls>"],
      "js":["content.js"],
      "css":["style.css"]
    }]

  }
